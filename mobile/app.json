{
  "expo": {
    "name": "Layerz Wallet",
    "slug": "layerzwallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ios_1024x1024.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.layerzwallet.mobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "icon": "./assets/images/ios_1024x1024.png",
      "icons": {
        "20": "./assets/images/ios_20x20.png",
        "29": "./assets/images/ios_29x29.png",
        "40": "./assets/images/ios_40x40.png",
        "60": "./assets/images/ios_60x60.png",
        "76": "./assets/images/ios_76x76.png",
        "80": "./assets/images/ios_80x80.png",
        "87": "./assets/images/ios_87x87.png",
        "120": "./assets/images/ios_120x120.png",
        "152": "./assets/images/ios_152x152.png",
        "167": "./assets/images/ios_167x167.png",
        "180": "./assets/images/ios_180x180.png",
        "1024": "./assets/images/ios_1024x1024.png"
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "permissions": ["android.permission.CAMERA"],
      "package": "com.layerzwallet.mobile",
      "icon": "./assets/images/android_512x512.png",
      "icons": [
        {
          "size": 48,
          "src": "./assets/images/android_48x48.png"
        },
        {
          "size": 72,
          "src": "./assets/images/android_72x72.png"
        },
        {
          "size": 96,
          "src": "./assets/images/android_96x96.png"
        },
        {
          "size": 144,
          "src": "./assets/images/android_144x144.png"
        },
        {
          "size": 192,
          "src": "./assets/images/android_192x192.png"
        },
        {
          "size": 512,
          "src": "./assets/images/android_512x512.png"
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/ios_1024x1024.png",
      "resizeMode": "native",
      "backgroundColor": "#000000"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ios_1024x1024.png",
          "resizeMode": "native",
          "backgroundColor": "#000000"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to scan QR codes with addresses"
        }
      ],
      "expo-secure-store",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "79e33289-7bd2-4ab4-8363-88ca65e99327"
      }
    },
    "owner": "layerztec"
  }
}
